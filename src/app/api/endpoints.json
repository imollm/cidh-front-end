{
  "baseUrl": "http://localhost:8080/",
  "headers": {
    "authorization": "Bearer %s",
    "accept": "application/json"
  },
  "endpoints": {
    "auth": {
      "register": {
        "uri": "/auth/register",
        "method": "POST"
      },
      "login": {
        "uri": "/auth/login",
        "method": "POST"
      },
      "logout": {
        "uri": "/auth/logout",
        "method": "GET"
      }
    }
  }
}
